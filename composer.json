{
    "name": "craftcamp/abac-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to wrap php-abac library into symfony applications",
    "keywords": ["access-control", "attributes", "security"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Axel Venet",
            "email": "avenet@kilix.fr",
            "homepage": "http://www.kilix.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "craftcamp/php-abac": "^3.0",
        "symfony/framework-bundle": "^3.0|^4.0"
    },
    "require-dev": {
        "symfony/console": "^3.0|^4.0",
        "symfony/phpunit-bridge": "^3.0|4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "CraftCamp\\AbacBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}